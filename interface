	interface animal
	{
		void sound();
		default void run()
		{
			System.out.println("Running");
		}
		static void eat()
		{
			System.out.println("Eatting");
		}
	}
	class Dog implements animal
	{
		public void sound()
		{
			System.out.println("Bow");
		}
	}
	public class problem1
	{
		public static void main(String args[])
		{
			Dog obj=new Dog();
			obj.sound();
	}
}
