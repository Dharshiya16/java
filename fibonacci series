
import java.util.Scanner;

public class problem1 {
	public static void main(String args[]) {
		int n=10;
		int first=0,second=1;
		int count=1;
		
		System.out.println("fibonacci series :" +n);

		while(count<=n) {
			System.out.println(first+"");
			int next=first+second;
			first=second;
			second=next;
			count++;
		
		}
	}

}
