interface print
{
	void display();
}
interface copy
{
	void show();
}
class xerox implements print,copy
{
	public void display()
	{
		System.out.println("Displaying");
	}
	public void show()
	{
		System.out.println("Showing");
	}
}
public class probleminheri {

	public static void main(String[] args) {
		xerox obj=new xerox();
		obj.show();
		obj.display();	 
	}
}
