interface sum
{
	int max=100;
	void show();
	static void display()
	{
		System.out.println("Displaying");
	}
}
class Myclass implements sum{
	public void show()
	{
		System.out.println("Showing");
	}
}
public class probleminter {
	public static void main(String[] args) {
		sum.display();
		Myclass obj=new Myclass();
		obj.show();
	}
}
